@prefix ex: <http://example.org/> .
@prefix vnclass: <http://example.org/vnclass/> .

vnclass:VNCLASS a vnclass:VNCLASS ;
    ex:hasChild vnclass:FRAMES,
        vnclass:MEMBERS,
        vnclass:SUBCLASSES,
        vnclass:THEMROLES ;
    vnclass:ID "push-12" .

vnclass:ARG a vnclass:ARG ;
    vnclass:type "Event",
        "PredSpecific",
        "ThemRole" ;
    vnclass:value "?Trajectory",
        "Agent",
        "Destination",
        "E",
        "Initial_Location",
        "Result",
        "Theme",
        "Trajectory",
        "e1",
        "e4",
        "ë",
        "ë2",
        "ë3" .

vnclass:ARGS a vnclass:ARGS ;
    ex:hasChild vnclass:ARG .

vnclass:DESCRIPTION a vnclass:DESCRIPTION ;
    vnclass:descriptionNumber "0.1",
        "0.2",
        "0.4",
        "1.2.7",
        "1.3" ;
    vnclass:primary "NP V NP",
        "NP V NP ADJP-Result",
        "NP V PP",
        "NP V PP-Conative",
        "NP V PP.trajectory" ;
    vnclass:secondary "Basic Transitive",
        "Conative",
        "NP-ADJP; Resultative",
        "PP",
        "PP; directional" ;
    vnclass:xtag "",
        "0.2",
        "through-PP" .

vnclass:EXAMPLE a vnclass:EXAMPLE ;
    ex:hasValue "Nora heaved the chair.",
        "Nora pushed at the wall.",
        "Nora pushed through the crowd.",
        "Nora yanked the button loose.",
        "She squeezed the toothpaste out." .

vnclass:EXAMPLES a vnclass:EXAMPLES ;
    ex:hasChild vnclass:EXAMPLE .

vnclass:FRAME a vnclass:FRAME ;
    ex:hasChild vnclass:DESCRIPTION,
        vnclass:EXAMPLES,
        vnclass:SEMANTICS,
        vnclass:SYNTAX .

vnclass:MEMBER a vnclass:MEMBER ;
    vnclass:features "+compression_of_patient",
        "+force_away_from_agent",
        "+force_toward_agent" ;
    vnclass:fn_mapping "Body_movement",
        "Cause_motion",
        "Cause_to_move_in_place",
        "Manipulation",
        "None" ;
    vnclass:grouping "",
        "heave.01",
        "jerk.01",
        "press.01",
        "pull.01",
        "push.01",
        "shove.01",
        "squeeze.03",
        "thrust.01 thrust.03",
        "yank.01 yank.02 yank.03" ;
    vnclass:name "heave",
        "jerk",
        "jostle",
        "nudge",
        "press",
        "pull",
        "push",
        "shove",
        "squeeze",
        "thrust",
        "tug",
        "yank" ;
    vnclass:verbnet_key "heave#2",
        "jerk#1",
        "jostle#1",
        "nudge#2",
        "press#1",
        "pull#5",
        "push#3",
        "shove#2",
        "squeeze#3",
        "thrust#2",
        "tug#2",
        "yank#2" ;
    vnclass:wn "heave%2:35:01",
        "jerk%2:35:01",
        "jostle%2:38:02 jostle%2:38:00",
        "nudge%2:35:00",
        "press%2:35:00 press%2:38:00",
        "pull%2:35:00 pull%2:35:04 pull%2:35:10",
        "push%2:38:00",
        "shove%2:38:00 shove%2:35:00 shove%2:38:01",
        "squeeze%2:35:03",
        "thrust%2:38:00 thrust%2:38:02",
        "tug%2:35:00 tug%2:35:04 tug%2:35:03 tug%2:35:02 tug%2:35:01",
        "yank%2:35:00" .

vnclass:NP a vnclass:NP ;
    ex:hasChild vnclass:SELRESTRS,
        vnclass:SYNRESTRS ;
    vnclass:value "Agent",
        "Result",
        "Theme",
        "Trajectory" .

vnclass:PRED a vnclass:PRED ;
    ex:hasChild vnclass:ARGS ;
    vnclass:bool "!" ;
    vnclass:value "cause",
        "contact",
        "equals",
        "exert_force",
        "has_location",
        "has_state",
        "motion",
        "overlaps" .

vnclass:PREP a vnclass:PREP ;
    ex:hasChild vnclass:SELRESTRS ;
    vnclass:value "at against on" .

vnclass:SELRESTR a vnclass:SELRESTR ;
    vnclass:Value "+" ;
    vnclass:type "animate",
        "concrete",
        "dir" .

vnclass:SEMANTICS a vnclass:SEMANTICS ;
    ex:hasChild vnclass:PRED .

vnclass:SYNRESTRS a vnclass:SYNRESTRS .

vnclass:SYNTAX a vnclass:SYNTAX ;
    ex:hasChild vnclass:NP,
        vnclass:PREP,
        vnclass:VERB .

vnclass:THEMROLE a vnclass:THEMROLE ;
    ex:hasChild vnclass:SELRESTRS ;
    vnclass:type "Agent",
        "Result",
        "Theme",
        "Trajectory" .

vnclass:VERB a vnclass:VERB .

vnclass:VNSUBCLASS a vnclass:VNSUBCLASS ;
    ex:hasChild vnclass:FRAMES,
        vnclass:MEMBERS,
        vnclass:SUBCLASSES,
        vnclass:THEMROLES ;
    vnclass:ID "push-12-1",
        "push-12-1-1" .

vnclass:FRAMES a vnclass:FRAMES ;
    ex:hasChild vnclass:FRAME .

vnclass:MEMBERS a vnclass:MEMBERS ;
    ex:hasChild vnclass:MEMBER .

vnclass:SUBCLASSES a vnclass:SUBCLASSES ;
    ex:hasChild vnclass:VNSUBCLASS .

vnclass:THEMROLES a vnclass:THEMROLES ;
    ex:hasChild vnclass:THEMROLE .

vnclass:SELRESTRS a vnclass:SELRESTRS ;
    ex:hasChild vnclass:SELRESTR .

